@using WebApplication1.csproj.Models

@model List<Employee>
@{
    ViewData["Title"] = "Index";
}

<h1>Index Bage</h1>

<table class="table-bordered table-hover">
    <tr>

        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
        <th></th>

    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Salary</td>
            <td><a href="/Employee/Edit/@item.Id">Edit</a></td>
        </tr>
    }
</table>
