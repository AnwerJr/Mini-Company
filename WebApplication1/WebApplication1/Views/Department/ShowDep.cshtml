@using WebApplication1.csproj.Models
@using WebApplication1.Models
@model List<Department>

@{
    ViewData["Title"] = "ShowDep";
}

<h1>ShowDep</h1>

<a href="/Department/Add" class="btn btn-success"> Add New</a>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Department ID</th>
            <th>Department Name</th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var dep in Model)
        {
            <tr>
                <td>@dep.Id</td>
                <td>@dep.Name</td>
                <td>
                   
                </td>
            </tr>
        }
	</tbody>
